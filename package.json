{
	"name": "steeprb",
	"displayName": "Steep",
	"description": "Steep type checker",
	"publisher": "soutaro",
	"license": "MIT",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/soutaro/steep-vscode.git"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:."
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "ruby",
				"filenames": [
					"Steepfile"
				]
			},
			{
				"id": "ruby-signature",
				"extensions": [
					".rbs"
				]
			}
		],
		"commands": [
			{
				"command": "steep.restartAll",
				"title": "Restart all",
				"category": "Steep"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2",
		"vsce": "^1.71.0"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1"
	}
}
